#include <stdio.h>

int main()

{

    int a,b,len,c=1;

    char str[100];

    printf("Enter a string :");

    scanf("%[^\n]%*c",str);

    

    len = strlen(str);

    printf("Duplicate caracters :");

     for (a = 0; a < len; a++) {

        for (b = a + 1; b < len; b++) 

        {

            if (str[a] == str[b]) 

            {

                if ( c == 1 && str[b] != '\0') 

                {

                    printf("%c", str[a]);

                }

                str[b] = '\0';

                c++;

            }

        }

        c = 1;

    }

    return 0;

}
